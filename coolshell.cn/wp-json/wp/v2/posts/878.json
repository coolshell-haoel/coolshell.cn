00a0\u00a0\u00a0\u00a0\u00a0 tpl_pack(tn,1);\r\n\u00a0\u00a0\u00a0 }\r\n\r\n\u00a0\u00a0\u00a0 tpl_dump(tn, TPL_FILE, &quot;users.tpl&quot;);\r\n\u00a0\u00a0\u00a0 tpl_free(tn);\r\n}\r\n<\/pre>\n<p>\u628a\u4e0a\u9762\u90a3\u4e2a\u5e8f\u5217\u5316\u5230\u6587\u4ef6\u7684\u201c\u5e8f\u53f7\u201d\u548c\u201c\u4eba\u540d\u201d\u53cd\u5e8f\u5217\u5316\u56de\u6765\u3002<\/p>\n<pre data-enlighter-language=\"c\" class=\"EnlighterJSRAW\">\r\n#include &quot;tpl.h&quot;\r\n\r\nint main(int argc, char *argv[]) {\r\n\u00a0\u00a0\u00a0 tpl_node *tn;\r\n\u00a0\u00a0\u00a0 int id;\r\n\u00a0\u00a0\u00a0 char *name;\r\n\r\n\u00a0\u00a0\u00a0 tn = tpl_map(&quot;A(is)&quot;, &amp;id, &amp;name);\r\n\u00a0\u00a0\u00a0 tpl_load(tn, TPL_FILE, &quot;users.tpl&quot;);\r\n\r\n\u00a0\u00a0\u00a0 while ( tpl_unpack(tn,1) &gt; 0 ) {\r\n\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0 printf(&quot;id %d, user %s\\n&quot;, id, name);\r\n\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0 free(name);\r\n\u00a0\u00a0\u00a0 }\r\n\u00a0\u00a0\u00a0 tpl_free(tn);\r\n}\r\n<\/pre>\n<p>\u66f4\u8be6\u7ec6\u7684\u4f7f\u7528\u8bf4\u660e\u8bf7\u770b\u5176\u6587\u6863\uff1a<br \/>\n<a href=\"http:\/\/tpl.sourceforge.net\/userguide.html\">http:\/\/tpl.sourceforge.net\/userguide.html<\/a><\/p>\n","raw":"","protected":false},"excerpt":{"rendered":"<p>tpl(http:\/\/tpl.sourceforge.net\/)\u662f\u4e00\u4e2a\u5f00\u6e90\u7684\u5c0f\u9879\u76ee\uff0c\u5176\u4e3b\u8981\u662f\u63d0\u4f9b\u4e00\u4e2a\u53ef\u4ee5\u5e8f\u5217\u5316\u6216\u53cd\u5e8f\u5217\u5316C\u8bed\u8a00\u6570\u636e\u7684\u4e00\u4e2aAPI\u51fd\u6570\u5e93\u3002tpl\u53f7\u79f0&#8230;<\/p>\n<p class=\"read-more\"><a class=\"btn btn-default\" href=\"https:\/\/coolshell.cn\/articles\/878.html\"> Read More<span class=\"screen-reader-text\">  Read More<\/span><\/a><\/p>\n","protected":false},"author":2,"featured_media":0,"comment_status":"open","ping_status":"open","sticky":false,"template":"","format":"standard","meta":[],"categories":[5,10,3],"tags":[59,130],"series":[],"a